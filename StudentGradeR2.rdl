    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Student_ID">
            <Value>=Parameters!Student_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select sa.St_id, c.Crs_Name , (sum (sa.St_Point)*100)/10 as Grades
from St_Answers as sa
inner join Exam as e on sa.Ex_Id =e.Ex_Id
inner join course as c on e.Crs_Id =c.Crs_Id
where sa.st_id=@Student_ID
group by sa.St_id, c.crs_Name
order by c.crs_Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
